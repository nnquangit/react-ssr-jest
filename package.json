{
  "name": "react-ssr-jest",
  "version": "1.0.0",
  "description": "Reactjs start kit integrating `reactjs`, `ssr`,`webpack 4`,`jest`,...",
  "main": "server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node server",
    "dev": "node server",
    "unit": "jest",
    "lint": "eslint --ext .js,.jsx src test/unit/specs",
    "lint:fix": "eslint --ext .js,.jsx src test/unit/specs --fix ",
    "build": "npm-run-all clean build:*",
    "build:client": "cross-env NODE_ENV=production webpack --config ./build/webpack.client.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config ./build/webpack.server.js --progress --hide-modules",
    "clean": "rimraf public/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nnquangit/react-ssr-jest.git"
  },
  "author": "nnquangit@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nnquangit/react-ssr-jest/issues"
  },
  "homepage": "https://github.com/nnquangit/react-ssr-jest#readme",
  "dependencies": {
    "@mdi/font": "^2.4.85",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.1.1",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "exstore": "^1.0.0",
    "firebase": "^5.1.0",
    "history": "^4.7.2",
    "jquery": "^3.3.1",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.10",
    "popper.js": "^1.14.3",
    "query-string": "^6.1.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-helmet": "^5.2.0",
    "react-router": "^4.3.1",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.3.1",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "ajv": "^6.5.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "compression-webpack-plugin": "^1.1.11",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.0.1",
    "eslint-config-standard": "^12.0.0-alpha.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "file-loader": "^1.1.11",
    "hard-source-webpack-plugin": "^0.7.5",
    "ignore-loader": "^0.1.2",
    "jest": "^23.1.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.1",
    "npm-run-all": "^4.1.3",
    "postcss-loader": "^2.1.5",
    "react-hot-loader": "^4.3.3",
    "react-test-renderer": "^16.4.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "ssr-plugin": "^1.0.0",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.3",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.3",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      ".(css|scss|less)$": "<rootDir>/test/unit/__mocks__/noneMock.js"
    },
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleDirectories": [
      "node_modules"
    ],
    "transformIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
